<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IProductTypeRepositoryService" />
                <binding name="BasicHttpBinding_IProductParameterRepositoryService" />
                <binding name="BasicHttpBinding_IProductParameterValueRepositoryService" />
                <binding name="BasicHttpBinding_IProductTypeRepositoryService1" />
                <binding name="BasicHttpBinding_IBuyerRepositoryService" />
                <binding name="BasicHttpBinding_IPurchaseRepositoryService" />
                <binding name="BasicHttpBinding_IPurchaseProductRepositoryService" />
            </basicHttpBinding>
          <wsDualHttpBinding>
            <binding name="WSDualHttpBinding_IProductRepositoryService" closeTimeout="00:01:00"
                openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
                bypassProxyOnLocal="false" transactionFlow="false" hostNameComparisonMode="StrongWildcard"
                maxBufferPoolSize="524288" maxReceivedMessageSize="65536"
                messageEncoding="Text" textEncoding="utf-8" useDefaultWebProxy="true">
              <readerQuotas maxDepth="32" maxStringContentLength="5242880" maxArrayLength="16384"
                  maxBytesPerRead="4096" maxNameTableCharCount="16384" />
              <reliableSession ordered="true" inactivityTimeout="00:10:00" />
              <security mode="Message">
                <message clientCredentialType="Windows" negotiateServiceCredential="true"
                    algorithmSuite="Default" />
              </security>
            </binding>
          </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:3595/ProductTypeRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProductTypeRepositoryService"
                contract="ProductTypeServiceRef.IProductTypeRepositoryService"
                name="BasicHttpBinding_IProductTypeRepositoryService" />
            <endpoint address="http://localhost:37948/ProductRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProductRepositoryService"
                contract="ProductServiceRef.IProductRepositoryService" name="BasicHttpBinding_IProductRepositoryService" />
            <endpoint address="http://localhost:23010/ProductParameterRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProductParameterRepositoryService"
                contract="ProductParameterServiceRefer.IProductParameterRepositoryService"
                name="BasicHttpBinding_IProductParameterRepositoryService" />
            <endpoint address="http://localhost:25643/ProductParameterValueRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProductParameterValueRepositoryService"
                contract="ProductParameterValueServiceReference.IProductParameterValueRepositoryService"
                name="BasicHttpBinding_IProductParameterValueRepositoryService" />
            <endpoint address="http://localhost:3595/ProductTypeRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProductTypeRepositoryService1"
                contract="ProductTypeServiceRef.IProductTypeRepositoryService"
                name="BasicHttpBinding_IProductTypeRepositoryService1" />
            <endpoint address="http://localhost:12533/BuyerRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IBuyerRepositoryService"
                contract="BuyerServiceRef.IBuyerRepositoryService" name="BasicHttpBinding_IBuyerRepositoryService" />
            <endpoint address="http://localhost:61367/PurchaseRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPurchaseRepositoryService"
                contract="PurchaseServiceRef.IPurchaseRepositoryService" name="BasicHttpBinding_IPurchaseRepositoryService" />
            <endpoint address="http://localhost:2880/PurchaseProductRepositoryService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPurchaseProductRepositoryService"
                contract="PurchaseProductServiceRef.IPurchaseProductRepositoryService"
                name="BasicHttpBinding_IPurchaseProductRepositoryService" />
            <endpoint address="http://localhost:37948/ProductRepositoryService.svc"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IProductRepositoryService"
                contract="ProductServiceRef.IProductRepositoryService" name="WSDualHttpBinding_IProductRepositoryService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>